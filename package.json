{
  "name": "kool-keyboards",
  "version": "0.1.0",
  "dependencies": {
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.4",
    "express": "*",
    "mongoose": "*",
    "passport": "*",
    "passport-local": "*",
    "cors": "*",
    "body-parser": "*"
  },
  "devDependencies": {
